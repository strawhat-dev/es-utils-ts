{
  "name": "es-utils-ts",
  "author": "strawhat-dev",
  "description": "esm-first simple typed utilities",
  "repository": "github:strawhat-dev/es-utils-ts",
  "version": "0.0.52",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./type-utils": {
      "types": "./dist/type-utils.d.ts"
    },
    "./externals": {
      "import": "./dist/externals.js",
      "types": "./dist/externals.d.ts"
    },
    "./common": {
      "import": "./dist/common/index.js",
      "types": "./dist/common/index.d.ts"
    },
    "./conditionals": {
      "import": "./dist/conditionals/index.js",
      "types": "./dist/conditionals/index.d.ts"
    },
    "./html": {
      "import": "./dist/html/index.js",
      "types": "./dist/html/index.d.ts"
    },
    "./objects": {
      "import": "./dist/objects/index.js",
      "types": "./dist/objects/index.d.ts"
    },
    "./path": {
      "import": "./dist/path/index.js",
      "types": "./dist/path/index.d.ts"
    }
  },
  "engines": {
    "node": ">=14.16"
  },
  "files": [
    "dist"
  ],
  "np": {
    "branch": "main",
    "tests": false
  },
  "scripts": {
    "build": "eslint && tsup",
    "rimraf": "pnpm -c dlx rimraf",
    "clean": "pnpm rimraf pnpm-lock.yaml node_modules"
  },
  "peerDependencies": {
    "deep-equal": "^2.2.1",
    "deepmerge-ts": "^5.1.0",
    "escape-string-regexp": "^5.0.0",
    "object-inspect": "^1.12.3",
    "rfdc": "^1.3.0",
    "type-fest": "^3.10.0"
  },
  "devDependencies": {
    "@types/deep-equal": "^1.0.1",
    "@types/eslint": "^8.37.0",
    "@types/node": "^20.1.4",
    "@types/object-inspect": "^1.8.1",
    "@types/path-browserify": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-tree-shaking": "^1.10.0",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
}
